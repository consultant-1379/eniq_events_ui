<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style>
        .dashboardContainer {
            z-index: 100;
            background-color: #fff;
            height:100%
        }

        .titlePanel {
            margin-top: 5px;
        }

        .mapPanel {
            height: 100%;
            margin-left:5px;
            margin-right:5px;
        }
    </ui:style>

    <g:AbsolutePanel ui:field="dashboardContainer" styleName="{style.dashboardContainer}">
        <g:FlowPanel ui:field="portletPanel">
            <g:HorizontalPanel ui:field="titlePanel" styleName="{style.titlePanel}">

            </g:HorizontalPanel>
        </g:FlowPanel>

        <g:SimplePanel ui:field="mapPanel" styleName="{style.mapPanel}"/>
    </g:AbsolutePanel>
</ui:UiBinder>