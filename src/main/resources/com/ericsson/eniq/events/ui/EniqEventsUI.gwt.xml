<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.3.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/tags/2.3.0/distro-source/core/src/gwt-module.dtd">
<module rename-to='EniqEventsUI'>
 
	 <!-- Inherit the core Web Toolkit stuff.  
	  (GWT not really needed as have GXT so may remove in time    -->
	   
	<inherits name='com.google.gwt.user.User'/>
	
	 <!-- Other module inherits     -->
	<inherits name="com.google.gwt.uibinder.UiBinder" />
	<inherits name="com.google.gwt.http.HTTP" />
	<!-- <inherits name="com.google.gwt.xml.XML"/> -->
    <inherits name="com.google.gwt.i18n.I18N"/>
	  	
	<!-- Inherit the default GWT style sheet.  You can change       -->
	<!-- the theme of your GWT application by uncommenting          -->
	<!-- any one of the following lines.                            -->
	  
	<inherits name='com.google.gwt.user.theme.standard.Standard'/> 
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> --> 
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->
	  
	<inherits name="com.extjs.gxt.ui.GXT" /> 
	<!-- <inherits name='com.extjs.gxt.charts.Chart'/> -->
	<inherits name="com.google.gwt.inject.Inject" />
	<inherits name='com.allen_sauer.gwt.dnd.gwt-dnd'/>
 	<inherits name="com.google.gwt.logging.Logging"/>
    <inherits name="com.google.web.bindery.autobean.AutoBean"/>
    <inherits name="org.gwtopenmaps.openlayers.OpenLayers"/>

    <!-- Eniq Shared Data Model -->
    <inherits name="com.ericsson.eniq.events.ui.EniqDataModel"/>    
    <!-- HighCharts Wrapper -->
    <inherits name="com.ericsson.eniq.events.highcharts.HighCharts"/>
    <inherits name='com.ericsson.eniq.events.common.Common'/>
    <inherits name='com.ericsson.eniq.events.widgets.Widgets'/>
    <inherits name="com.ericsson.eniq.events.sessionbrowser.SessionBrowser"/>
    <inherits name="com.ericsson.eniq.events.kpianalysis.KpiAnalysis"/> 
    <!-- Specify the app entry point class.                         -->
	<entry-point class='com.ericsson.eniq.events.ui.client.mvp.MainEntryPoint'/>
	
		<!-- Specify the paths for translatable code. Exclude Unit tests -->
	<source path='client'>
		<exclude name="**/*Test.java" />
	</source>

	<!-- Configuration for standard GWT logging - should not be enabled by default -->
 	<set-property name="gwt.logging.enabled" value="FALSE"/>
  	<set-property name="gwt.logging.logLevel" value="INFO"/>
  	<!-- consoleHandler prints logging to console... -->
  	<set-property name="gwt.logging.consoleHandler" value="DISABLED" />
  	<!-- developmentModeHandler prints logging to Eclipse Development Mode view... -->
  	<set-property name="gwt.logging.developmentModeHandler" value="ENABLED"/>
  	<set-property name="gwt.logging.popupHandler" value="DISABLED"/>
  	<set-property name="gwt.logging.systemHandler" value="DISABLED"/>
 	<set-property name="gwt.logging.simpleRemoteHandler" value="DISABLED"/>
  	<set-property name="gwt.logging.firebugHandler" value="DISABLED"/>

	<!-- GWT 2.3+ seems to like this   -->
	<set-configuration-property name="UiBinder.useSafeHtmlTemplates" value="true" /> 

    <!-- ie6 and opera are excluded to speed-up the compilation process since they are not supported-->
    <set-property name="user.agent" value="ie8, ie9, gecko1_8, safari"/>

	<!-- Generate a registry of modules for Eniq Events app -->
	<generate-with
		class="com.ericsson.eniq.events.ui.generator.ModuleRegistryGenerator">
		<when-type-assignable
			class="com.ericsson.eniq.events.ui.client.main.IEniqEventsModuleRegistry" />
	</generate-with>
	<generate-with class="com.ericsson.eniq.events.common.client.generator.PropertiesInfoGenerator">
		<when-type-assignable class="com.ericsson.eniq.events.common.client.datatype.IPropertiesInfo"/>
	</generate-with>
    <replace-with class="com.ericsson.eniq.events.ui.client.EniqEventsServiceProperties">
        <when-type-is class="com.ericsson.eniq.events.common.client.service.IServiceProperties"/>
    </replace-with>
</module>